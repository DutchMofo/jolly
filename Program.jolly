
int main()
{
	AnalyseTest a;
	
	a.count = a.number = 10;
	
	swapCountNumber(&a);
	
	return 0;
}

AnalyseTest* swapCountNumber(AnalyseTest* a)
{
	a.(count, number, inner.data) = a.(number, count, inner.data);
	return a;
}

struct AnalyseTest
{
	int count;
	int number;
	Inner.InnerInner inner;
}

struct Inner
{
	struct InnerInner
	{
		int data;
	}
}