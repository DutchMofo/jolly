
int main()
{
	AnalyseTest a;
	Fancy b;
	b = Fancy {
		id: 0xDEADBEEF,
		awwYis: .5,
		count: 0,
		number: 1,
	};
	
	a.count = a.number = 10;
		
	if(1) {
		swapCountNumber(&a);
	} else {
		return 1;
	}
	return 0;
}

AnalyseTest* swapCountNumber(AnalyseTest* b)
{
	b.(count, number) = b.(number, count);
	return b;
}

struct StructID
{
	int id;
}

struct AnalyseTest : StructID
{
	int count;
	int number;
}

struct Fancy : AnalyseTest
{
	float awwYis;
}