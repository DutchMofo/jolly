
int main()
{
	AnalyseTest a;
	
	a.count = 10;
	a.number = 1;
	swapCountNumber(&a);
	
	int b = 0;
	int c = 19;
	
	return 0;
}

bool, AnalyseTest* swapCountNumber(AnalyseTest* a)
{
	a.(count, number, inner.data) = a.(number, count, inner.data);
	return true, a;
}

struct AnalyseTest
{
	int count;
	int number;
	Inner inner;
}

struct Inner
{
	int data;
}