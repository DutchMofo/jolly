
int main()
{
	AnalyseTest a = {
		id: ID.ANALYSE_TEST,
		count: 10,
		number: 12,
	};
	
	swapCountNumber(&a);
	return !~10;
}

AnalyseTest* swapCountNumber(AnalyseTest* b)
{
	b.(count, number) = b.(number, count);
	return b;
}

enum ID
{
	ANALYSE_TEST,
}

struct StructID
{
	ID id;
}

struct AnalyseTest : StructID
{
	int count;
	int number;
}