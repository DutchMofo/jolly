
int main()
{
	AnalyseTest a = {
		id: 1,
		count: 10,
		number: 12,
	};
	
	swapCountNumber(&a);
	return true;
}

AnalyseTest* swapCountNumber(AnalyseTest* b)
{
	b.(count, number) = b.(number, count);
	return b;
}

struct StructID
{
	int id;
}

struct AnalyseTest : StructID
{
	int count;
	int number;
}