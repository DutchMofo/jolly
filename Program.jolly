
int main()
{
	AnalyseTest a, b = a;
	a.count = a.number = 10;
	
	
	swapCountNumber(&a);
	
	return 0;
}

AnalyseTest* swapCountNumber(AnalyseTest* b)
{
	b.(count, number, inner.data) = b.(number, count, inner.data);
	return b;
}

struct AnalyseTest
{
	int count;
	int number;
	Inner.InnerInner inner;
}

struct Inner
{
	struct InnerInner
	{
		int data;
	}
}